<%= form_for @prior, url: buyer_prior_policies_path do |f| %>
<div class="form-group">
  <%= f.label :pp_exists %>
  <%= f.select :pp_exists, ["Yes", "No"] %>
</div><br />
<div class="form-group">
    <%= f.label "Prior Policy" %><br />
    <%= f.file_field :pp_copy %>
</div><br />
<div class='cant-be-seen'>
  <%= f.number_field :buyer_id, value: @buyer.id %>
</div>
<div class="actions">
    <%= f.submit class: "btn btn-primary btn-lg" %>
</div>
<% end %>
