<%= form_for @occupancy, url: buyer_occupancies_path do |f| %>
<div class="form-group">
  <%= f.label :occupants %>
  <%= f.select :occupants, ["Owner", "Tenant", "Unimproved"] %>
</div><br />
<div class='cant-be-seen'>
  <%= f.number_field :buyer_id, value: @buyer.id %>
</div>
<div class="actions">
    <%= f.submit class: "btn btn-primary btn-lg" %>
</div>
<% end %>
